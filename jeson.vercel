{
  "version": 2,
  "builds": [
    {
      "src": "ai_studio_code.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/ai_studio_code.html"
    },
    {
      "src": "/(.*)",
      "dest": "/ai_studio_code.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self' https://fonts.googleapis.com https://fonts.gstatic.com https://www.gstatic.com https://firebase.googleapis.com https://*.firebaseio.com https://*.firebaseapp.com; script-src 'self' 'unsafe-inline' https://www.gstatic.com; style-src 'self' 'unsafe-inline' https://fonts.googleapis.com; font-src 'self' https://fonts.gstatic.com; connect-src 'self' https://*.firebaseio.com https://*.googleapis.com wss://*.firebaseio.com; media-src 'self' blob:;"
        }
      ]
    }
  ]
}